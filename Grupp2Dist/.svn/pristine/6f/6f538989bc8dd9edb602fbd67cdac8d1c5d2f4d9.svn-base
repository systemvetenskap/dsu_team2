/*----------------------Reset-------------------------*/
html, main, container, body, div, span, h1, h2, h3, h4, h5, h6, p, ol, ul, li,
h6, p, ol, ul, li,
blockquote, pre, form, lable, legend, table, caption, thead, tbody, tfoot,
tr, th, td, article, aside, canvas, details, embed, figure, figcaption,
footer, header, hgroup, menu, nav, output, section, summary, audio, video {
    margin: 0;
    padding: 0;
    border: 0;
    font-size: 80%;
    font: inherit;
    vertical-align: baseline;
    font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
}

body {
    margin: 0;
    padding: 0;
    background-image: url(images/background2.jpg);
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

/*-----------------Header--------------------*/
div.header{
    background-color: #3b7895;
    height: 180px;
    background-image: url(images/Banner.png);
}

div.header img{
    float: left;
    height: 180px;
    max-height: 165px;
}

div.header h1{
    font-size: 4em;
    color: white;
    float: left;
    padding: 30px 0 0 30px;
}

div.container {
    width: 70%;
    margin-left: auto;
    margin-right: auto;
    padding-left: 0px;
    padding-right: 0px;
    background: #FFF;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
}

