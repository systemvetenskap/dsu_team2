/*-----------------Navigation------------------*/


div.localNavigation, div.localNavigation2{
    display: none;
    position: absolute;
    margin-left: 0px;
    height: 100%;
    margin-top: 0px;
    width: 9%;
    min-width: 120px;
    float:left;
    margin-left: 1%;
}

div.localNavigation ul, div.localNavigation2 ul{
    list-style-type: none;
    margin: 0;
    padding: 0;
    width: 100%;
    background-color: #f1f1f1;
    
}

div.localNavigation li a, div.localNavigation2 li a {
    display: block;
    color: #000;
    padding: 8px 0 8px 16px;
    text-decoration: none;
}

div.localNavigation a.active, div.localNavigation2 a.active {
    background-color: #4CAF50;
    color: white;
}

div.localNavigation li a:hover:not(.active), div.localNavigation2 li a:hover:not(.active){
    background-color: #555;
    color: white;
}




